{
    "name": "tinyhouse-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.2.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/graphql": "^14.5.0",
        "@types/node": "^12.12.47",
        "@types/react": "^16.9.37",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "antd": "^4.3.4",
        "apollo-boost": "^0.4.9",
        "graphql": "^15.1.0",
        "moment": "^2.27.0",
        "react": "^16.13.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1",
        "typescript": "^3.7.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --coverage",
        "eject": "react-scripts eject",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile='schema.json' --includes='src/**/*.ts' --globalTypesFile='./src/lib/graphql/globalTypes.ts' --target=typescript",
        "storybook": "start-storybook -p 9001 -c .storybook"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@apollo/react-testing": "^3.1.4",
        "@storybook/react": "^5.3.19",
        "@types/enzyme": "^3.10.5",
        "@types/jest": "^24.9.1",
        "@types/storybook-readme": "^5.0.4",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "awesome-typescript-loader": "^5.2.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "markdown-loader": "^5.1.0",
        "prettier": "^2.0.5",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "storybook-readme": "^5.0.8",
        "ts-jest": "^26.1.1"
    },
    "proxy": "http://localhost:9000",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
